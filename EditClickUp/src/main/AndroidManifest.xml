<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.fish.myapplication">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme">
        <activity
            android:name=".MainActivity"
            android:label="@string/app_name">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".Default2Resize"
            android:windowSoftInputMode="adjustUnspecified">

        </activity>
        <activity android:name=".AdjustPan"></activity>
        <activity
            android:name="com.AdjustResize.AdjustResize"
            android:windowSoftInputMode="adjustResize"></activity>


        <activity android:name=".AdjustResizeHiddenActivity"
            android:windowSoftInputMode="stateHidden|adjustResize"></activity>

    </application>

</manifest>
